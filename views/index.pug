extends /layouts/_bootstrap
append styles
	// Custom styles for this template
	link(href='../css/dashboard.css', rel='stylesheet')
block body  
	nav.navbar.navbar-inverse.navbar-fixed-top
		.container-fluid
			.navbar-header
				button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='#navbar', aria-expanded='false', aria-controls='navbar')
					span.sr-only Toggle navigation
					span.icon-bar
					span.icon-bar
					span.icon-bar
				a.navbar-brand(href='#') Project name 
			#navbar.navbar-collapse.collapse
				ul.nav.navbar-nav.navbar-right
					li
						a(href='#') Dashboard
					li
						a(href='#') Settings
					li
						a(href='#') Profile
					li
						a(href='#') Help
				form.navbar-form.navbar-right
					input.form-control(type='text', placeholder='Search...')
	.container-fluid
		.row
			.col-sm-3.col-md-2.sidebar
				ul.nav.nav-sidebar
					li.active
						a(href='#')
							| Overview 
							span.sr-only (current)
					li
						a(href='#') Reports
					li
						a(href='#') Analytics
					li
						a(href='#') Export
				h4.sub-header Users
				ul.nav.nav-sidebar
					each user in users
						li
							a(href=`/chat/${user.id}`)= user.username
				ul.nav.nav-sidebar
					li
						a(href='') Nav item again
					li
						a(href='') One more nav
					li
						a(href='') Another nav item
			.col-sm-9.col-sm-offset-3.col-md-10.col-md-offset-2.main
				h3.welcome-title Welcome 
					span.welcome-span #{username} !
				h1.page-header Dashboard
				//- pre= JSON.stringify(users, null, 2)
				div.dashboard-container
					iframe(title="Report Section" src="https://app.powerbi.com/view?r=eyJrIjoiMDQ0OTA2M2QtYTE1NS00NmZiLThmMWUtYTVhMmJkOTFhOGI2IiwidCI6ImMwNmZiNTU5LTFiNjgtNGI4NC1hMTRmLTQ3ZDBkODM3YTVhYiIsImMiOjR9" frameborder="0" allowFullScreen="true")
				h2.sub-header Users information üìÑ
				.table-responsive
					table.table.table-striped
						thead
							tr
								each prop in Object.getOwnPropertyNames(users[0].dataValues)
									th= prop
						tbody
							each user in users
								tr
									td= user.id
									td= user.username
									td= user.email
									td Inaccesible üîê
									td= new Date(user.createdAt).toLocaleString()
									td= new Date(user.updatedAt).toLocaleString()